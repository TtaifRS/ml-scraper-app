{
  "appId": "com.taif-ML.xing-scraper",
  "files": ["dist-electron", "dist-react"],
  "icon": "./xing-icon.png",
  "asar": "false",
  "extraResources": [
   {
    "from":  "dist-electron/preload.cjs",
    "to": "app/dist-electron/preload.cjs"
   },
   {
    "from": ".env",
    "to": "app/.env"
   }
  ],
  "mac": {
    "target": "dmg"
  },
  "linux": {
    "target": "AppImage",
    "category": "Utility"
  },
  "win": {
    "target": ["portable", "msi"]
  }
}