{
  "appId": "com.taif-ML.xing-scraper",
  "productName": "Xing-scraper",
  "files": ["dist-electron", "dist-react"],
  "icon": "./xing-icon.png",
  "extraResources": [
   {
    "from":  "dist-electron/preload.cjs",
    "to": "app/dist-electron/preload.cjs"
   },
   {
    "from": ".env",
    "to": ".env"
   }
  ],
  "mac": {
    "icon": "./xing-icon.icns",
    "target": "dmg"
  },
  "linux": {
    "target": "AppImage",
    "category": "Utility"
  },
  "win": {
    "target": ["portable", "msi"],
    "arch": ["ia32", "x64"]
  },
  "buildVersion": "1.0.0",
  "asar": "false",
  "compression": "normal"
}